{
	"name": "HistViz",
	"version": "1.0",
	"manifest_version": 2,
	"description": "History Visualization extension (this is just a testing board version)",
	"browser_action": {
	},
	"background": {
		"scripts": ["jquery.min.js", "background.js"]
	},
	"content_scripts": [
		{
			"matches": ["http://*/*"],
			"js": ["jquery.min.js", "graph_test.js"]
		}
	],
	"permissions": [
		"http://api.flickr.com/",
		"http://ajax.googleapis.com/",
		"tabs",
		"history"
	]
}
